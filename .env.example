# Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env file with actual credentials to version control

# MySQL Database Configuration
MYSQL_ROOT_PASSWORD=your_secure_root_password_here
MYSQL_DATABASE=crud_flask
MYSQL_USER=dev
MYSQL_PASSWORD=your_secure_user_password_here

# Flask Application Configuration
SECRET_KEY=your_random_secret_key_at_least_32_chars_long
FLASK_ENV=production

# Database Connection (used by Flask app)
DB_HOST=phonebook-mysql
DB_USER=dev
DB_PASSWORD=your_secure_user_password_here
DB_NAME=crud_flask

# Docker Hub Configuration (for CI/CD)
DOCKERHUB_USERNAME=your_dockerhub_username

# AWS EC2 Configuration (for deployment)
EC2_HOST=your_ec2_public_ip_or_dns
EC2_USER=ubuntu
EC2_SSH_KEY_PATH=/path/to/your/private/key.pem
